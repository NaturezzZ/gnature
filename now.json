{
  "version": 2,
  "name": "mirror",
  "regions": [
        "hnd1",
        "icn1",
        "sin1"
  ],
  "build": {
    "env": {
      "GO111MODULE": "on"
    }
  },
  "functions": {
    "api/index.go": {
      "maxDuration": 5
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/index.go"
    }
  ]
}
